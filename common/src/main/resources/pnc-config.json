{
"@class":"ModuleConfigJson","name":"pnc-config",
        "configs":[
          {
            "@module-config":"jenkins-build-driver",
            "url":"${env.PNC_JENKINS_URL}",
            "username":"${env.PNC_JENKINS_USERNAME}",
            "password":"${env.PNC_JENKINS_PASSWORD}"
          },
          {
            "@module-config":"maven-repo-driver",
            "baseUrl":"${env.PNC_APROX_URL}"
          },
          {
            "@module-config":"docker-environment-driver",
            "ip":"${env.PNC_DOCKER_IP}",
            "inContainerUser":"${env.PNC_DOCKER_CONT_USER}",
            "inContainerUserPassword":"${env.PNC_DOCKER_CONT_PASSWORD}"
          }
        ]
}
